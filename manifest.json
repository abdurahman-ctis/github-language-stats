{
    "manifest_version": 2,
   
    "name": "Github Languages",
    "version": "0.1.0",
    "description": "Show language stats of a Github user.",
   
    "content_scripts": [{
      "js": ["jquery-3.3.1.min.js", "Chart.min.js", "content.js"],
      "matches": ["https://github.com/*/"]
    }],

    "background":
    {
        "scripts": ["jquery-3.3.1.min.js"]
    }
   
  }